<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="applicationusers.usercontroller">
   <center>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="adminic" fitHeight="100.0" fitWidth="100.0" layoutX="73.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../Downloads/admin.png" />
               </image></ImageView>
            <ImageView fx:id="buyeric" fitHeight="100.0" fitWidth="100.0" layoutX="233.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../Downloads/buyer.png" />
               </image></ImageView>
            <ImageView fx:id="selleric" fitHeight="100.0" fitWidth="100.0" layoutX="410.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../Downloads/seller.png" />
               </image></ImageView>
            <Button fx:id="adminlogin" layoutX="73.0" layoutY="213.0" mnemonicParsing="false" onAction="#changeToAdminLoginPage" prefHeight="26.0" prefWidth="100.0" text="Admin" />
            <Button fx:id="buyerlogin" layoutX="233.0" layoutY="213.0" mnemonicParsing="false" onAction="#changeToRegisterPage" prefHeight="26.0" prefWidth="100.0" text="Buyer" />
            <Button fx:id="sellerlogin" layoutX="410.0" layoutY="213.0" mnemonicParsing="false" onAction="#changeToRegisterPage" prefHeight="26.0" prefWidth="100.0" text="Seller" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
